package aula_5;

public class Aluno implements Aula{
    //ATRIBUTOS
     private String nome;
     private int idade;
     private String ra;
     private String cpf;
     private String curso;
     private double notaAV1;
     private double notaAV2; 
     
    @Override
    public void av1(double a) {
        if (a>=0 && a<=10.0){
            this.notaAV1 = a;
        }  else {
           this.notaAV1 = 0;
        }
        
        if (a>=5.5 && a<=6.0){
            this.notaAV1=6.0;
        }
    }

    @Override
    public void av2(double a) {
        if (a>=0 && a<=10.0){
            this.notaAV2 = a;
        } else {
            this.notaAV2 = 0;
        }
        
        if (a>=5.5 && a<=6.0){
            this.notaAV2=6.0;
        }
    }

    @Override
    public void entradaCatraca() {
    }
    
    public void exibirNotas(){
        System.out.printf("Notas do aluno: \n");
        System.out.printf("Nota AV1 = %.2f\n", this.notaAV1);
        System.out.printf("Nota AV2 = %.2f\n", this.notaAV2);
    }
  
     
     //MÃ‰TODO CONSTRUTOR (NOME E CPF)
     public Aluno(String nome, String cpf){
        this.nome = nome;
        this.cpf = cpf;
     }
     
     //GETTER
     public String getNome(){
         return nome;
     }
     public String getCpf(){
         return cpf;
     }
     public String getRa(){
         return ra;
     }
     public String getCurso(){
         return curso;
     }
     public int getIdade(){
         return idade;
     }
     
     //SETTER
     public void setNome(String nome){
         this.nome = nome;
     }
     public void setCpf(String cpf){
         this.cpf = cpf;
     }
     public void setRa(String ra){
         this.ra = ra;
     }
     public void setCurso(String curso){
         this.curso = curso;
     }
     public void setIdade(int idade){
         this.idade = idade;
     }

}
